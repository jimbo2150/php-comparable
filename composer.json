{
    "name": "jimbo2150/php-comparable",
    "description": "PHP library to allow for comparison of two objects with a comparison operator.",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Jimbo2150\\PhpComparable\\": "src/"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Jimbo2150\\PhpComparable\\Tests\\": "tests/"
        }
	},
    "authors": [
        {
            "name": "Jim Robinson",
            "email": "jimbo2150@gmail.com"
        }
    ],
    "require": {
		"php": ">=8.1",
        "jimbo2150/php-utilities": "^1.0"
	},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.69",
        "phpunit/phpunit": "^10.0|^11.0|^12.0"
    }
}
